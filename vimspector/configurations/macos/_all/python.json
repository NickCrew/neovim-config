{
  "default": true,
  "adapter": "debugpy",
  "filetypes": [
    "python"
  ],
  "breakpoints": {
    "exception": {
      "all": "N",
      "caught": "N",
      "uncaught": "Y"
    }
  },
  "configuration": {
    "type": "python",
    "request": "launch",
    "stopOnEntry": true,
    "python": "python",
    "program": "${file}"
  }
}
